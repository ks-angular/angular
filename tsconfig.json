{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@uni-logic/components": ["/components"],
      "@uni-logic/components/*": ["/components/*"],
      "@uni-logic/editor": ["/editor"],
      "@uni-logic/editor/*": ["/editor/*"],
      "@uni-logic/engine": ["/engine"],
      "@uni-logic/engine/*": ["/engine/*"]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
